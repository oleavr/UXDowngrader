<UserControl x:Class="UXDowngrader.ProcessPicker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="35">
    <Grid>
        <Image Name="crosshair" Source="/UXDowngrader;component/Resources/ccrosshair.png" MouseDown="crosshair_MouseDown" MouseUp="crosshair_MouseUp" MouseMove="crosshair_MouseMove" GotMouseCapture="crosshair_GotMouseCapture" LostMouseCapture="crosshair_LostMouseCapture" HorizontalAlignment="Center" VerticalAlignment="Top" Width="63" Height="63" Margin="0,-13,0,0" />
        <TextBlock Name="infoTextBlock" HorizontalAlignment="Center" VerticalAlignment="Top" TextTrimming="CharacterEllipsis" Margin="0,35,0,0" />
    </Grid>
</UserControl>
